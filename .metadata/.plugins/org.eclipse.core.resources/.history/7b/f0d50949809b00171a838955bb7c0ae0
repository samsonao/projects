package com.samson.test;

@RestController
@EnableAutoConfiguration

public class Example {
	@RestController
	@EnableAutoConfiguration
	public class Example {

	    @RequestMapping("/")
	    String home() {
	        return "Hello World!";
	    }

	    public static void main(String[] args) throws Exception {
	        SpringApplication.run(Example.class, args);
	    }

}
